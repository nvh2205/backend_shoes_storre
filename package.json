{
  "name": "api_app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "devstart": "nodemon --inspect ./bin/www",
    "serverstart": "DEBUG=api_app:* npm run devstart",
    "lint:fix": "eslint --ignore-path .gitignore --ignore-pattern public --fix .",
    "generate:apidoc": "apidoc -i ./routes -o ./public/apidocs",
    "migrate": "node_modules/.bin/sequelize db:migrate",
    "seed": "node_modules/.bin/sequelize db:seed:all"
  },
  "dependencies": {
    "apidoc": "^0.27.1",
    "bcrypt": "^5.0.1",
    "body-parser": "^1.19.0",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^8.2.0",
    "ejs": "^3.1.6",
    "express": "~4.16.1",
    "express-session": "^1.17.1",
    "express-validator": "^6.6.0",
    "http-errors": "~1.6.3",
    "http-status-codes": "^1.4.0",
    "jsonwebtoken": "^8.5.1",
    "mailparser": "^3.1.0",
    "method-override": "^3.0.0",
    "moment": "^2.27.0",
    "morgan": "~1.9.1",
    "multer": "^1.4.2",
    "mysql2": "^2.2.5",
    "node-cron": "^2.0.3",
    "nodemailer": "^6.5.0",
    "nodemon": "^2.0.4",
    "pug": "2.0.0-beta11",
    "rotating-file-stream": "^2.1.1",
    "sequelize": "^6.3.5",
    "sequelize-cli": "^6.2.0"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "eslint": "^7.3.1",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.0",
    "prettier": "^2.0.5"
  }
}
